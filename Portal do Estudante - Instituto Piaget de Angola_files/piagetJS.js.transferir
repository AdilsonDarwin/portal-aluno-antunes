let teams = [];
$("#teams").keyup(function (event) {
    $('.typeahead').typeahead('destroy');
    $("#teams").focus();
    $.ajax({
        type: 'GET',
        dataType: 'json',
        url: "/ajax/getusersbyvalue",
        data: { "qvalue": $("#teams").val() },
        success: function (json) {
            $('.typeahead').typeahead('destroy');
            teams = [];
            $("#teams").focus();
            if (json.length > 0) {
                teams = [];
                $('.typeahead').typeahead('open');
                let arrayLength = json.length;
                for (let i = 0; i < arrayLength; i++) {
                    if (!teams.includes(json[i]['Nome'])) {
                        teams.push(json[i]['Nome']);
                    }
                }
                $('#nba-teams .typeahead').typeahead({
                    hint: true,
                    highlight: true,
                    minLength: 1
                },
                    {
                        name: 'teams',
                        source: substringMatcher(teams)
                    });
                $("#teams").focus();
                return teams;
            }
            $("#teams").focus();
        }
    });
    $("#teams").focus();

});
let substringMatcher = function (strs) {
    return function findMatches(q, cb) {
        let matches, substringRegex;
        matches = [];
        substrRegex = new RegExp(q, 'i');
        $.each(strs, function (i, str) {
            if (substrRegex.test(str)) {
                matches.push(str);
            }
        });
        cb(matches);
    };
};
